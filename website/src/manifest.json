{
    "manifest_version": 2,
    "short_name": "prokey-link",
    "name": "prokey-link",
    "start_url": ".",
    "display": "standalone",
    "theme_color": "#000000",
    "background_color": "#ffffff",
    "icons": [{
        "src": "favicon.ico",
        "sizes": "64x64 32x32 24x24 16x16",
        "type": "image/x-icon"
    }],
    "permissions": [
        "tabs",
        "notifications",
        "webRequest",
        "*"
    ]
}