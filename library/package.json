{
  "name": "link",
  "version": "0.1.0",
  "description": "Connect your website or extension to prokey hardware wallet",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "tsc",
    "watch": "tsc-watch ",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "tslint -p tsconfig.json",
    "test": "jest --config jestconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "files": [
    "lib/**/*"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/prokey-io/prokey-link.git"
  },
  "keywords": [
    "prokey",
    "hardware-wallet",
    "bitcoin",
    "ethereum",
    "ripple"
  ],
  "author": "Ali Akbar Mohammadi",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/prokey-io/prokey-link/issues"
  },
  "homepage": "https://github.com/prokey-io/prokey-link#readme",
  "devDependencies": {
    "@types/chrome": "0.0.143",
    "@types/jest": "^26.0.23",
    "@types/pump": "^1.1.1",
    "jest": "^27.0.6",
    "prettier": "^2.3.2",
    "ts-jest": "^27.0.3",
    "tsc-watch": "^4.4.0",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.3.5"
  },
  "dependencies": {
    "extensionizer": "^1.0.1",
    "obj-multiplex": "^1.0.0",
    "pump": "^3.0.0"
  }
}
